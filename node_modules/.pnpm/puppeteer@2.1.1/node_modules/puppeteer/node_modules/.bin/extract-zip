#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../extract-zip@1.7.0/node_modules/extract-zip/cli.js" "$@"
else
  exec node  "$basedir/../../../../../extract-zip@1.7.0/node_modules/extract-zip/cli.js" "$@"
fi
