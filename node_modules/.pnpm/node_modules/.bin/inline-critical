#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/inline-critical@11.0.1/node_modules/inline-critical/node_modules:/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/inline-critical@11.0.1/node_modules:/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/inline-critical@11.0.1/node_modules/inline-critical/node_modules:/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/inline-critical@11.0.1/node_modules:/Users/aal/Projects/Appmanschap/Critical-CSS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../inline-critical/cli.js" "$@"
else
  exec node  "$basedir/../inline-critical/cli.js" "$@"
fi
